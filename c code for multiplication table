
#include <stdio.h>



int main() {
    int num,ans;

    printf("Enter the number to generate its multiplication table: ");
    scanf("%d", &num);

    printf("Multiplication Table for %d:\n", num);

    for (int i = 1; i <= 10; i++) 
    {
        ans=num * i;
        printf("%d x %d = %d\n", num, i, ans);
    }

    return 0;
}
