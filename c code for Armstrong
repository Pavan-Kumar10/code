
#include <stdio.h>
 armstrong(int num)
{
    int arm=((num%10)(num%10)(num%10))+(((num%100)/10)((num%100)/10)((num%100)/10))+(((num%1000)/100)((num%1000)/100)((num%1000)/100));
    if (arm==num)
    {
        printf("The %d is a armstrong number.",num);
    }
    else
    {
        printf("The %d is not a armstrong number.",num);
    }
}
int armstrong(num);


int main() {
   
    int num1;
    printf("Enter a number: ");
    scanf("%d",&num1);
    armstrong(num1);
    

    return 0;
}
